####     M6577.metrics.base_distribution_by_cycle.pdf
####     M6577.metrics.base_distribution_by_cycle_metrics
####     M6577.metrics.insert_size_histogram.pdf
####     M6577.metrics.insert_size_metrics
####     M6577.metrics.quality_by_cycle.pdf
####     M6577.metrics.quality_by_cycle_metrics
####     M6577.metrics.quality_distribution.pdf
####     M6577.metrics.quality_distribution_metrics

/usr/picard/docker_helper.sh CollectMultipleMetrics R=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta I=M6577.bam O=M6577.metrics


