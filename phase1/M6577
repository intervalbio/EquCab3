gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I M6577.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O M6577.recalibration.table


########
#### stage: samtools_unmapped
####   inputs:
####     M6577.bai (from apply_bsqr)
####     M6577.bam (from apply_bsqr)
####   outputs:
####     M6577.unmapped.fq.gz

samtools view  -u -b -f 4 M6577.bam | samtools fastq - | gzip -c - > M6577.unmapped.fq.gz


########
#### stage: intervalbio_depthofcoverage
####   inputs:
####     M6577.bai (from apply_bsqr)
####     M6577.bam (from apply_bsqr)
####   outputs:
####     M6577.coverage.tsv

samtools depth M6577.bam | depth-of-coverage.py > M6577.coverage.tsv


########
#### stage: analyze_covariates
####   inputs:
####     firstpass.table (from base_recalibration_table)
####     secondpass.table (from base_recalibration_table_2)
####   outputs:
####     M6577.BQSR.csv
####     M6577.BQSR.pdf

gatk AnalyzeCovariates  -before firstpass.table -after secondpass.table -csv M6577.BQSR.csv -plots M6577.BQSR.pdf


