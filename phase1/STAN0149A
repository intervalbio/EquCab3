####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz.tbi
####     STAN0149A.bai (from apply_bsqr)
####     STAN0149A.bam (from apply_bsqr)
####   outputs:
####     STAN0149A.recalibration.table

gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I STAN0149A.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O STAN0149A.recalibration.table


########
#### stage: analyze_covariates
####   inputs:
####     firstpass.table (from base_recalibration_table)
####     secondpass.table (from base_recalibration_table_2)
####   outputs:
####     STAN0149A.BQSR.csv
####     STAN0149A.BQSR.pdf

gatk AnalyzeCovariates  -before firstpass.table -after secondpass.table -csv STAN0149A.BQSR.csv -plots STAN0149A.BQSR.pdf



