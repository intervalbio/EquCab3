####   inputs:
####     STAN0149A.bai (from apply_bsqr)
####     STAN0149A.bam (from apply_bsqr)
####   outputs:
####     STAN0149A.unmapped.fq.gz

samtools view  -u -b -f 4 STAN0149A.bam | samtools fastq - | gzip -c - > STAN0149A.unmapped.fq.gz


########
#### stage: platypus_callvariants
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.fai
####     STAN0149A.bai (from apply_bsqr)
####     STAN0149A.bam (from apply_bsqr)
####   outputs:
####     STAN0149A.platypuscallvariants.vcf.gz
####     STAN0149A.platypuscallvariants.vcf.gz.tbi

Platypus.py callVariants --refFile=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta --bamFiles=STAN0149A.bam --output=STAN0149A.platypuscallvariants.vcf ; bgzip STAN0149A.platypuscallvariants.vcf ; tabix -p vcf STAN0149A.platypuscallvariants.vcf.gz


########
#### stage: base_recalibration_table_2
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
