gatk ApplyBQSR -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I YAKU0170A.bam --bqsr-recal-file YAKU0170A.recalibration.table -O YAKU0170A.recalibrated.bam


