Platypus.py callVariants --refFile=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta --bamFiles=M8463.bam --output=M8463.platypuscallvariants.vcf ; bgzip M8463.platypuscallvariants.vcf ; tabix -p vcf M8463.platypuscallvariants.vcf.gz


