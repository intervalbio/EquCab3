samtools merge -@ `nproc` M8463.merged.bam M8463_C7N4YANXX_1.bam M8463_C7N4YANXX_2.bam M8463_C7N4YANXX_3.bam M8463_C7N4YANXX_4.bam M8463_C7N4YANXX_5.bam ; samtools index M8463.merged.bam M8463.merged.bai


########
#### stage: mark_duplicates
####   inputs:
####     M8463.bam (from merge_bam)
####   outputs:
####     M8463.markduplicates.bam
####     M8463.markduplicates.metrics.txt

/usr/picard/docker_helper.sh MarkDuplicates I=M8463.bam O=M8463.markduplicates.bam M=M8463.markduplicates.metrics.txt


########
#### stage: index_mark_duplicates
####   inputs:
####     M8463.bam (from mark_duplicates)
####   outputs:
####     M8463.bai

samtools index -@ `nproc` M8463.bam M8463.bai


########
#### stage: base_recalibration_table
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz.tbi
####     M8463.bai (from index_mark_duplicates)
####     M8463.bam (from mark_duplicates)
####   outputs:
####     M8463.recalibration.table

gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I M8463.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O M8463.recalibration.table


########
#### stage: apply_bsqr
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     M8463.bai (from index_mark_duplicates)
