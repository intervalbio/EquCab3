####   outputs:
####     TWILIGHT.platypuscallvariants.vcf.gz
####     TWILIGHT.platypuscallvariants.vcf.gz.tbi

Platypus.py callVariants --refFile=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta --bamFiles=TWILIGHT.bam --output=TWILIGHT.platypuscallvariants.vcf ; bgzip TWILIGHT.platypuscallvariants.vcf ; tabix -p vcf TWILIGHT.platypuscallvariants.vcf.gz



