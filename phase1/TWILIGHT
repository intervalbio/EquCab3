####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.ann
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.bwt
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.fai
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.pac
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.sa
####     TWILIGHT_C11WHACXX_2GTATCT_1.fq.gz (from trim_C11WHACXX_2GTATCT)
####     TWILIGHT_C11WHACXX_2GTATCT_2.fq.gz (from trim_C11WHACXX_2GTATCT)
####   outputs:
####     TWILIGHT_C11WHACXX_2GTATCT.sorted.bai
####     TWILIGHT_C11WHACXX_2GTATCT.sorted.bam

bwa mem -M -t `nproc` -R '@RG\tID:C11WHACXX.2GTATCT\tLB:C11WHACXX.2GTATCT\tPL:ILLUMINA\tPU:C11WHACXX.2GTATCT.UNKNOWN\tSM:TWILIGHT' GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta TWILIGHT_C11WHACXX_2GTATCT_1.fq.gz TWILIGHT_C11WHACXX_2GTATCT_2.fq.gz | samtools view -b - | samtools sort -@ `nproc` -o TWILIGHT_C11WHACXX_2GTATCT.sorted.bam -; samtools index TWILIGHT_C11WHACXX_2GTATCT.sorted.bam TWILIGHT_C11WHACXX_2GTATCT.sorted.bai


########
#### stage: merge_bam
####   inputs:
####     TWILIGHT_C11WHACXX_2ACGCAT.bam (from bwa_C11WHACXX_2ACGCAT)
####     TWILIGHT_C11WHACXX_2CGATGT.bam (from bwa_C11WHACXX_2CGATGT)
####     TWILIGHT_C11WHACXX_2GTATCT.bam (from bwa_C11WHACXX_2GTATCT)
####     TWILIGHT_C11WHACXX_2TAGATG.bam (from bwa_C11WHACXX_2TAGATG)
####     TWILIGHT_C11WHACXX_3ACGCAT.bam (from bwa_C11WHACXX_3ACGCAT)
####     TWILIGHT_C11WHACXX_3CGATGT.bam (from bwa_C11WHACXX_3CGATGT)
####     TWILIGHT_C11WHACXX_3GTATCT.bam (from bwa_C11WHACXX_3GTATCT)
####     TWILIGHT_C11WHACXX_3TAGATG.bam (from bwa_C11WHACXX_3TAGATG)
####   outputs:
####     TWILIGHT.merged.bai
####     TWILIGHT.merged.bam

samtools merge -@ `nproc` TWILIGHT.merged.bam TWILIGHT_C11WHACXX_2ACGCAT.bam TWILIGHT_C11WHACXX_3ACGCAT.bam TWILIGHT_C11WHACXX_2GTATCT.bam TWILIGHT_C11WHACXX_3GTATCT.bam TWILIGHT_C11WHACXX_2CGATGT.bam TWILIGHT_C11WHACXX_2TAGATG.bam TWILIGHT_C11WHACXX_3CGATGT.bam TWILIGHT_C11WHACXX_3TAGATG.bam ; samtools index TWILIGHT.merged.bam TWILIGHT.merged.bai


########
#### stage: mark_duplicates
####   inputs:
####     TWILIGHT.bam (from merge_bam)
####   outputs:
####     TWILIGHT.markduplicates.bam
####     TWILIGHT.markduplicates.metrics.txt

/usr/picard/docker_helper.sh MarkDuplicates I=TWILIGHT.bam O=TWILIGHT.markduplicates.bam M=TWILIGHT.markduplicates.metrics.txt


########
#### stage: index_mark_duplicates
####   inputs:
####     TWILIGHT.bam (from mark_duplicates)
####   outputs:
####     TWILIGHT.bai

samtools index -@ `nproc` TWILIGHT.bam TWILIGHT.bai


########
#### stage: base_recalibration_table
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz.tbi
####     TWILIGHT.bai (from index_mark_duplicates)
####     TWILIGHT.bam (from mark_duplicates)
####   outputs:
####     TWILIGHT.recalibration.table

gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I TWILIGHT.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O TWILIGHT.recalibration.table


########
#### stage: apply_bsqr
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     TWILIGHT.bai (from index_mark_duplicates)
####     TWILIGHT.bam (from mark_duplicates)
####     TWILIGHT.recalibration.table (from base_recalibration_table)
####   outputs:
####     TWILIGHT.recalibrated.bai
####     TWILIGHT.recalibrated.bam

gatk ApplyBQSR -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I TWILIGHT.bam --bqsr-recal-file TWILIGHT.recalibration.table -O TWILIGHT.recalibrated.bam


########
#### stage: samtools_unmapped
####   inputs:
####     TWILIGHT.bai (from apply_bsqr)
####     TWILIGHT.bam (from apply_bsqr)
####   outputs:
####     TWILIGHT.unmapped.fq.gz

samtools view  -u -b -f 4 TWILIGHT.bam | samtools fastq - | gzip -c - > TWILIGHT.unmapped.fq.gz


########
#### stage: base_recalibration_table_2
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz.tbi
####     TWILIGHT.bai (from apply_bsqr)
####     TWILIGHT.bam (from apply_bsqr)
####   outputs:
####     TWILIGHT.recalibration.table

gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I TWILIGHT.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O TWILIGHT.recalibration.table


########
#### stage: gatk_haplotypcaller
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
