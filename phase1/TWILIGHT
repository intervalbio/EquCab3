####     TWILIGHT.metrics.insert_size_histogram.pdf
####     TWILIGHT.metrics.insert_size_metrics
####     TWILIGHT.metrics.quality_by_cycle.pdf
####     TWILIGHT.metrics.quality_by_cycle_metrics
####     TWILIGHT.metrics.quality_distribution.pdf
####     TWILIGHT.metrics.quality_distribution_metrics

/usr/picard/docker_helper.sh CollectMultipleMetrics R=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta I=TWILIGHT.bam O=TWILIGHT.metrics


########
#### stage: analyze_covariates
####   inputs:
####     firstpass.table (from base_recalibration_table)
####     secondpass.table (from base_recalibration_table_2)
####   outputs:
####     TWILIGHT.BQSR.csv
####     TWILIGHT.BQSR.pdf

gatk AnalyzeCovariates  -before firstpass.table -after secondpass.table -csv TWILIGHT.BQSR.csv -plots TWILIGHT.BQSR.pdf


########
#### stage: bcftools_mpileup
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.fai
####     TWILIGHT.bai (from apply_bsqr)
####     TWILIGHT.bam (from apply_bsqr)
####   outputs:
####     TWILIGHT.samtoolsmpileup.vcf.gz
####     TWILIGHT.samtoolsmpileup.vcf.gz.tbi

bcftools mpileup -Ou -f GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta TWILIGHT.bam | bcftools call -Ou -mv | bcftools norm -f GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta -Oz -o TWILIGHT.samtoolsmpileup.vcf.gz ; tabix TWILIGHT.samtoolsmpileup.vcf.gz


########
#### stage: intervalbio_depthofcoverage
####   inputs:
####     TWILIGHT.bai (from apply_bsqr)
####     TWILIGHT.bam (from apply_bsqr)
####   outputs:
####     TWILIGHT.coverage.tsv

samtools depth TWILIGHT.bam | depth-of-coverage.py > TWILIGHT.coverage.tsv


########
#### stage: platypus_callvariants
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta
