gatk ApplyBQSR -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I M6188.bam --bqsr-recal-file M6188.recalibration.table -O M6188.recalibrated.bam


########
#### stage: samtools_unmapped
####   inputs:
####     M6188.bai (from apply_bsqr)
####     M6188.bam (from apply_bsqr)
####   outputs:
####     M6188.unmapped.fq.gz

samtools view  -u -b -f 4 M6188.bam | samtools fastq - | gzip -c - > M6188.unmapped.fq.gz


########
#### stage: gatk_haplotypcaller
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
