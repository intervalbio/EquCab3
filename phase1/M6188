samtools merge -@ `nproc` M6188.merged.bam M6188_C6R6KANXX_1.bam M6188_C6R6KANXX_2.bam M6188_C6R6KANXX_3.bam M6188_C6R6KANXX_4.bam M6188_C6R6KANXX_5.bam ; samtools index M6188.merged.bam M6188.merged.bai


########
#### stage: mark_duplicates
####   inputs:
####     M6188.bam (from merge_bam)
####   outputs:
####     M6188.markduplicates.bam
####     M6188.markduplicates.metrics.txt

/usr/picard/docker_helper.sh MarkDuplicates I=M6188.bam O=M6188.markduplicates.bam M=M6188.markduplicates.metrics.txt


########
#### stage: index_mark_duplicates
####   inputs:
####     M6188.bam (from mark_duplicates)
####   outputs:
####     M6188.bai

samtools index -@ `nproc` M6188.bam M6188.bai


########
#### stage: base_recalibration_table
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
