samtools merge -@ `nproc` M6105.merged.bam M6105_C6R6KANXX_1.bam M6105_C6R6KANXX_2.bam M6105_C6R6KANXX_3.bam M6105_C6R6KANXX_4.bam M6105_C6R6KANXX_5.bam ; samtools index M6105.merged.bam M6105.merged.bai


########
#### stage: mark_duplicates
####   inputs:
####     M6105.bam (from merge_bam)
####   outputs:
####     M6105.markduplicates.bam
####     M6105.markduplicates.metrics.txt

/usr/picard/docker_helper.sh MarkDuplicates I=M6105.bam O=M6105.markduplicates.bam M=M6105.markduplicates.metrics.txt


########
#### stage: index_mark_duplicates
####   inputs:
####     M6105.bam (from mark_duplicates)
####   outputs:
####     M6105.bai

samtools index -@ `nproc` M6105.bam M6105.bai


########
#### stage: base_recalibration_table
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz.tbi
####     M6105.bai (from index_mark_duplicates)
####     M6105.bam (from mark_duplicates)
####   outputs:
####     M6105.recalibration.table

gatk BaseRecalibrator  -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I M6105.bam --known-sites GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.vcf.gz -O M6105.recalibration.table


########
#### stage: apply_bsqr
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa.fai
####     M6105.bai (from index_mark_duplicates)
####     M6105.bam (from mark_duplicates)
####     M6105.recalibration.table (from base_recalibration_table)
####   outputs:
####     M6105.recalibrated.bai
####     M6105.recalibrated.bam

gatk ApplyBQSR -R GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa -I M6105.bam --bqsr-recal-file M6105.recalibration.table -O M6105.recalibrated.bam


########
#### stage: platypus_callvariants
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta.fai
####     M6105.bai (from apply_bsqr)
####     M6105.bam (from apply_bsqr)
####   outputs:
####     M6105.platypuscallvariants.vcf.gz
####     M6105.platypuscallvariants.vcf.gz.tbi

Platypus.py callVariants --refFile=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta --bamFiles=M6105.bam --output=M6105.platypuscallvariants.vcf ; bgzip M6105.platypuscallvariants.vcf ; tabix -p vcf M6105.platypuscallvariants.vcf.gz


########
#### stage: intervalbio_depthofcoverage
####   inputs:
####     M6105.bai (from apply_bsqr)
####     M6105.bam (from apply_bsqr)
####   outputs:
####     M6105.coverage.tsv

samtools depth M6105.bam | depth-of-coverage.py > M6105.coverage.tsv


########
#### stage: base_recalibration_table_2
####   inputs:
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.dict
####     GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fa
