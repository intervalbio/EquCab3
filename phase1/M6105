/usr/picard/docker_helper.sh CollectMultipleMetrics R=GCF_002863925.1_EquCab3.0_genomic/GCF_002863925.1_EquCab3.0_genomic.fasta I=M6105.bam O=M6105.metrics


########
#### stage: samtools_unmapped
####   inputs:
####     M6105.bai (from apply_bsqr)
####     M6105.bam (from apply_bsqr)
####   outputs:
####     M6105.unmapped.fq.gz

samtools view  -u -b -f 4 M6105.bam | samtools fastq - | gzip -c - > M6105.unmapped.fq.gz


########
#### stage: analyze_covariates
####   inputs:
####     firstpass.table (from base_recalibration_table)
####     secondpass.table (from base_recalibration_table_2)
####   outputs:
